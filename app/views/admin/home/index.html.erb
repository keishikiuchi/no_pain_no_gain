<div class="col-xs-offset-2 col-xs-8">
	<h1>会員一覧</h1>
	<table class="table table-condensed">

		<tbody>
			<% @users.each do |user| %>
				<tr>
					<td><%= attachment_image_tag user, :profile_image, :fill, 40, 40, fallback: "no_image.jpg", size:'50x50' %></td>
					<td><%= user.name %></td>
					<td><%= link_to "ユーザーページへ", user_path(user.id) %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>